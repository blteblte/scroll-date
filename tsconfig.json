{
  "compileOnSave": false,
  "compilerOptions": {
    "moduleResolution": "node",
    "removeComments": true,
    "sourceMap": true,
    "noUnusedLocals": true,
    "pretty": true,
    "target": "es5",
    "lib": [
      "es2017",
      "es5",
      "es2015",
      "dom"
    ],
    "typeRoots": [
      "node_modules/@types",
      "./src/ts/typings"
    ]
  },
  "exclude": [
    "node_modules",
    "wwwroot",
    "bin",
    "obj",
    "Views",
    "Scripts"
  ]
}
